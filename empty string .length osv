import java.util.Arrays;

public class Classroom {
  
  public static void main(String[] args){
    String[] students = {"Sade", "Alexus", "Sam", "Koma"};
    double[] mathScores = new double[4];  //empty array
    mathScores[0] = 94.5;  //store value in the 0th index
    mathScores[2] = 76.8;
    System.out.println("The number of students in the class is " + students.length);  //skriver ut hur m√•nga element som finns i students
    
  }
}
